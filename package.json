{
  "name": "globeview",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/styles/style.css -o ./public/style.css",
    "watch:css": "npx tailwindcss -i ./src/styles/style.css -o ./public/style.css --watch",
    "dev": "concurrently \"npm run watch:css\" \"vite\"",
    "build": "npm run build:css && vite build",
    "preview": "npm run build:css && vite preview",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/cli": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tailwindcss": "^4.1.11",
    "vite": "^7.0.4"
  }
}
